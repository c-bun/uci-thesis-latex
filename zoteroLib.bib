  file = {/Users/colinrathbun/Zotero/storage/UIDKKDHL/Zhao et al. - 2016 - Long non-coding RNA SNHG5 suppresses gastric cance.pdf;/Users/colinrathbun/Zotero/storage/DQDTVBAU/onc2016110.html}
}

@article{TanakaRitsukoIntronicU50small2001,
