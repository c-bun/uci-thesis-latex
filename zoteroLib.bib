  file = {/Users/colinrathbun/Zotero/storage/VYTGW45U/Liu et al. - 2018 - Statistical Coupling Analysis-Guided Library Desig.pdf;/Users/colinrathbun/Zotero/storage/3L2QHQ9L/acs.biochem.html}
}

