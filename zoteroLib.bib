  abstract = {Genetically encoded fluorescent protein tags have revolutionized proteome studies, whereas the lack of intrinsically fluorescent RNAs has hindered transcriptome exploration. Among several RNA\textendash{}fluorophore complexes that potentially address this problem, RNA Mango has an exceptionally high affinity for its thiazole orange (TO)-derived fluorophore, TO1\textendash{}Biotin (Kd $\sim$3 nM), and, in complex with related ligands, it is one of the most redshifted fluorescent macromolecular tags known. To elucidate how this small aptamer exhibits such properties, which make it well suited for studying low-copy cellular RNAs, we determined its 1.7-\AA-resolution co-crystal structure. Unexpectedly, the entire ligand, including TO, biotin and the linker connecting them, abuts one of the near-planar faces of the three-tiered G-quadruplex. The two heterocycles of TO are held in place by two loop adenines and form a 45$^\circ$ angle with respect to each other. Minimizing this angle would increase quantum yield and further improve this tool for in vivo RNA visualization.},
  language = {en},
  number = {7},
  journal = {Nature Chemical Biology},
  author = {Iii, Robert J. Trachman and Demeshkina, Natalia A. and Lau, Matthew W. L. and Panchapakesan, Shanker Shyam S. and Jeng, Sunny C. Y. and Unrau, Peter J. and Ferr{\'e}-D'Amar{\'e}, Adrian R.},
  month = jul,
  year = {2017},
  pages = {807-813},
  file = {/Users/colinrathbun/Zotero/storage/HBT66L83/Iii et al. - 2017 - Structural basis for high-affinity fluorophore bin.pdf;/Users/colinrathbun/Zotero/storage/9STY6QI9/nchembio.html}
}

@article{WarnerStructuralbasisactivity2014,
  title = {Structural Basis for Activity of Highly Efficient {{RNA}} Mimics of Green Fluorescent Protein},
