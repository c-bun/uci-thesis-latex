  pages = {3565-3570},
  file = {/Users/colinrathbun/Zotero/storage/3RK6TPEB/Proc. Natl. Acad. Sci. U.S.A. 1997 Shah.pdf}
}

@article{LosHaloTagnovelprotein2008,
  title = {{{HaloTag}}: A Novel Protein Labeling Technology for Cell Imaging and Protein Analysis.},
  volume = {3},
