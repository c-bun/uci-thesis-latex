  journal = {Proceedings of the National Academy of Sciences of the United States of America},
  author = {Mahdavi, Alborz and Szychowski, Janek and Ngo, John T and Sweredoski, Michael J and Graham, Robert L J and Hess, Sonja and Schneewind, Olaf and Mazmanian, Sarkis K and Tirrell, David A},
