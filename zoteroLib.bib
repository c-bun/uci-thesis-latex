  keywords = {cytoplasm,degradation,long noncoding RNA,ribosome,ribosome profiling,translation,transposable element},
  pages = {867-882},
  file = {/Users/colinrathbun/Zotero/storage/97CMCRRD/Carlevaro-Fita et al. - 2016 - Cytoplasmic long noncoding RNAs are frequently bou.pdf;/Users/colinrathbun/Zotero/storage/YWY22LIR/867.html},
  pmid = {27090285}
}

