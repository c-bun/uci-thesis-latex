@article{RN27,
author = {Sellmyer, Mark A and Bronsart, Laura and Imoto, Hiroshi and Contag, Christopher H and Wandless, Thomas J and Prescher, Jennifer A},
title = {{Visualizing cellular interactions with a generalized proximity reporter.}},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
year = {2013},
volume = {110},
number = {21},
pages = {8567--8572},
month = may,
publisher = {National Acad Sciences},
affiliation = {Department of Chemical and Systems Biology, Stanford University, Stanford, CA 94305, USA.},
doi = {10.1073/pnas.1218336110},
pmid = {23650381},
pmcid = {PMC3666728},
language = {English},
read = {Yes},
rating = {0},
date-added = {2016-07-08T01:54:31GMT},
date-modified = {2017-05-20T01:10:18GMT},
abstract = {Interactions among neighboring cells underpin many physiological processes ranging from early development to immune responses. When these interactions do not function properly, numerous pathologies, including infection and cancer, can result. Molecular imaging technologies, especially optical imaging, are uniquely suited to illuminate complex cellular interactions within the context of living tissues in the body. However, no tools yet exist that allow the detection of microscopic events, such as two cells coming into close proximity, on a global, whole-animal scale. We report here a broadly applicable, longitudinal strategy for probing interactions among cells in living subjects. This approach relies on the generation of bioluminescent light when two distinct cell populations come into close proximity, with the intensity of the optical signal correlating with relative cellular location. We demonstrate the ability of this reporter strategy to gauge cell-cell proximity in culture models in vitro and then evaluate this approach for imaging tumor-immune cell interactions using a murine breast cancer model. In these studies, our imaging strategy enabled the facile visualization of features that are otherwise difficult to observe with conventional imaging techniques, including detection of micrometastatic lesions and potential sites of tumor immunosurveillance. This proximity reporter will facilitate probing of numerous types of cell-cell interactions and will stimulate the development of similar techniques to detect rare events and pathological processes in live animals.},
url = {http://www.pnas.org/content/110/21/8567.abstract},
local-url = {file://localhost/Users/colinrathbun/Documents/Library.papers3/Articles/2013/Sellmyer/Proceedings%20of%20the%20National%20Academy%20of%20Sciences%20of%20the%20United%20States%20of%20America%202013%20Sellmyer-1.pdf},
file = {{Proceedings of the National Academy of Sciences of the United States of America 2013 Sellmyer-1.pdf:/Users/colinrathbun/Documents/Library.papers3/Articles/2013/Sellmyer/Proceedings of the National Academy of Sciences of the United States of America 2013 Sellmyer-1.pdf:application/pdf;Proceedings of the National Academy of Sciences of the United States of America 2013 Sellmyer-1.pdf:/Users/colinrathbun/Documents/Library.papers3/Articles/2013/Sellmyer/Proceedings of the National Academy of Sciences of the United States of America 2013 Sellmyer-1.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1073/pnas.1218336110}}
}

