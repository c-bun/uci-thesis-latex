file = {{Proceedings of the National Academy of Sciences of the United States of America 2013 Islam.pdf:/Users/colinrathbun/Documents/Library.papers3/Articles/2013/Islam/Proceedings of the National Academy of Sciences of the United States of America 2013 Islam.pdf:application/pdf;Proceedings of the National Academy of Sciences of the United States of America 2013 Islam.pdf:/Users/colinrathbun/Documents/Library.papers3/Articles/2013/Islam/Proceedings of the National Academy of Sciences of the United States of America 2013 Islam.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1073/pnas.1216365110}}
}

