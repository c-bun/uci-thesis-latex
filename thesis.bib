@article{Branchini:2011ep,
author = {Branchini, Bruce R and Rosenberg, Justin C and Ablamsky, Danielle M and Taylor, Kelsey P and Southworth, Tara L and Linder, Samantha J},
title = {{Sequential bioluminescence resonance energy transfer-fluorescence resonance energy transfer-based ratiometric protease assays with fusion proteins of firefly luciferase and red fluorescent protein}},
journal = {Analytical Biochemistry},
year = {2011},
volume = {414},
number = {2},
pages = {239--245},
affiliation = {Department of Chemistry, Connecticut College, 270 Mohegan Avenue, New London, CT 06320, USA.},
doi = {10.1016/j.ab.2011.03.031},
pmid = {21453669},
language = {English},
rating = {0},
date-added = {2015-02-09T18:23:43GMT},
date-modified = {2017-04-27T20:19:11GMT},
abstract = {We report here the preparation of ratiometric luminescent probes that contain two well-separated emission peaks produced by a sequential bioluminescence resonance energy transfer (BRET)-fluorescence resonance energy transfer (FRET) process. The probes are single soluble fusion proteins consisting of a thermostable firefly luciferase variant that catalyze yellow-green (560nm maximum) bioluminescence and a red fluorescent protein covalently labeled with a near-infrared fluorescent dye. The two proteins are connected by a decapeptide containing a protease recognition site specific for factor Xa, thrombin, or caspase 3. The rates of protease cleavage of the fusion protein substrates were monitored by recording emission spectra and plotting the change in peak ratios over time. Detection limits of 0.41nM for caspase 3, 1.0nM for thrombin, and 58nM for factor Xa were realized with a scanning fluorometer. Our results demonstrate for the first time that an efficient sequential BRET-FRET energy transfer process based on firefly luciferase bioluminescence can be employed to assay physiologically important protease activities.},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0003269711002028},
uri = {\url{papers3://publication/doi/10.1016/j.ab.2011.03.031}}
}

