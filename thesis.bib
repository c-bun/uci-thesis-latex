@article{Matikonda:2015kh,
author = {Matikonda, Siddharth S and Orsi, Douglas L and Staudacher, Verena and Jenkins, Imogen A and Fiedler, Franziska and Chen, Jiayi and Gamble, Allan B},
title = {{Bioorthogonal prodrug activation driven by a strain-promoted 1,3-dipolar cycloaddition}},
journal = {Chem. Sci.},
year = {2015},
volume = {6},
number = {2},
pages = {1212--1218},
month = jan,
publisher = {The Royal Society of Chemistry},
doi = {10.1039/C4SC02574A},
language = {English},
read = {Yes},
rating = {0},
date-added = {2015-03-26T02:43:45GMT},
date-modified = {2017-04-27T20:19:11GMT},
abstract = { Due to the formation of hydrolysis-susceptible adducts, the 1,3-dipolar cycloaddition between an azide and strained trans-cyclooctene (TCO) has been disregarded in the field of bioorthogonal chemistry. We report a method which uses the instability of the adducts to our advantage in a prodrug activation strategy. The reaction of trans-cyclooctenol (TCO-OH) with a model prodrug resulted in a rapid 1,3-dipolar cycloaddition with second-order rates of 0.017 M{\textminus}1 s{\textminus}1 and 0.027 M{\textminus}1 s{\textminus}1 for the equatorial and axial isomers, respectively, resulting in release of the active compound. 1H NMR studies showed that activation proceeded via a triazoline and imine, both of which are rapidly hydrolyzed to release the model drug. Cytotoxicity of a doxorubicin prodrug was restored in vitro upon activation with TCO-OH, while with cis-cyclooctenol (CCO-OH) no activation was observed. The data also demonstrates the potential of this reaction in organic synthesis as a mild orthogonal protecting group strategy for amino and hydroxyl groups.},
url = {http://pubs.rsc.org/en/content/articlehtml/2015/sc/c4sc02574a},
local-url = {file://localhost/Users/colinrathbun/Documents/Library.papers3/Articles/2015/Matikonda/Chem.%20Sci.%202015%20Matikonda.pdf},
file = {{Chem. Sci. 2015 Matikonda.pdf:/Users/colinrathbun/Documents/Library.papers3/Articles/2015/Matikonda/Chem. Sci. 2015 Matikonda.pdf:application/pdf;Chem. Sci. 2015 Matikonda.pdf:/Users/colinrathbun/Documents/Library.papers3/Articles/2015/Matikonda/Chem. Sci. 2015 Matikonda.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1039/C4SC02574A}}
}

