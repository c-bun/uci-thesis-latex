@article{Baldwin:2016bh,
author = {Baldwin, Robert L and Rose, George D},
title = {{How the hydrophobic factor drives protein folding.}},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
year = {2016},
volume = {113},
number = {44},
pages = {12462--12466},
month = nov,
affiliation = {Department of Biochemistry, Stanford University Medical Center, Beckman Center, School of Medicine, Stanford, CA 94305-5307; baldwinb@stanford.edu.},
doi = {10.1073/pnas.1610541113},
pmid = {27791131},
pmcid = {PMC5098675},
language = {English},
rating = {0},
date-added = {2017-06-08T18:28:23GMT},
date-modified = {2017-08-23T01:29:22GMT},
abstract = {How hydrophobicity (HY) drives protein folding is studied. The 1971 Nozaki-Tanford method of measuring HY is modified to use gases as solutes, not crystals, and this makes the method easy to use. Alkanes are found to be much more hydrophobic than rare gases, and the two different kinds of HY are termed intrinsic (rare gases) and extrinsic (alkanes). The HY values of rare gases are proportional to solvent-accessible surface area (ASA), whereas the HY values of alkanes depend on special hydration shells. Earlier work showed that hydration shells produce the hydration energetics of alkanes. Evidence is given here that the transfer energetics of alkanes to cyclohexane [Wolfenden R, Lewis CA, Jr, Yuan Y, Carter CW, Jr (2015) Proc Natl Acad Sci USA 112(24):7484-7488] measure the release of these shells. Alkane shells are stabilized importantly by van der Waals interactions between alkane carbon and water oxygen atoms. Thus, rare gases cannot form this type of shell. The very short (approximately picoseconds) lifetime of the van der Waals interaction probably explains why NMR efforts to detect alkane hydration shells have failed. The close similarity between the sizes of the opposing energetics for forming or releasing alkane shells confirms the presence of these shells on alkanes and supports Kauzmann's 1959 mechanism of protein folding. A space-filling model is given for the hydration shells on linear alkanes. The model reproduces the n values of Jorgensen et al. [Jorgensen WL, Gao J, Ravimohan C (1985) J Phys Chem 89:3470-3473] for the number of waters in alkane hydration shells.},
url = {http://www.pnas.org/lookup/doi/10.1073/pnas.1610541113},
local-url = {file://localhost/Users/colinrathbun/Documents/Library.papers3/Articles/2016/Baldwin/Proceedings%20of%20the%20National%20Academy%20of%20Sciences%20of%20the%20United%20States%20of%20America%202016%20Baldwin.pdf},
file = {{Proceedings of the National Academy of Sciences of the United States of America 2016 Baldwin.pdf:/Users/colinrathbun/Documents/Library.papers3/Articles/2016/Baldwin/Proceedings of the National Academy of Sciences of the United States of America 2016 Baldwin.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1073/pnas.1610541113}}
}

