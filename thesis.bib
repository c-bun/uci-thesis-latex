@article{Obexer:2017db,
author = {Obexer, Richard and Walport, Louise J and Suga, Hiroaki},
title = {{Exploring sequence space: harnessing chemical and biological diversity towards new peptide leads.}},
journal = {Curr. Opin. Chem. Biol.},
year = {2017},
volume = {38},
pages = {52--61},
month = mar,
affiliation = {Department of Chemistry, Graduate School of Science, The University of Tokyo, 7-3-1 Hongo, Bunkyo-ku, Tokyo 113-0033, Japan.},
doi = {10.1016/j.cbpa.2017.02.020},
pmid = {28319812},
language = {English},
rating = {0},
date-added = {2017-06-08T18:39:31GMT},
date-modified = {2017-08-23T01:29:21GMT},
abstract = {From their early roots in natural products, peptides now represent an expanding class of novel drugs. Their modular structures make them ideal candidates for pooled library screening approaches. Key technologies for library generation and screening, such as SICLOPPS, phage display and mRNA display, give unparalleled access to tight binding peptides. Through combination with genetic code reprogramming and chemical modifications, access to more natural product-like libraries, spanning non-canonical peptide space, is readily achievable. Recent advances in these fields enable introduction of diverse non-standard motifs, such as cyclisation and backbone methylations. Peptide discovery platforms now allow robust access to potent, highly functionalised peptides against virtually any protein of interest, with typical binding constants in the nanomolar range. Application of these optimised platforms in a drug discovery setting has the potential to significantly accelerate identification of new leads.},
url = {http://linkinghub.elsevier.com/retrieve/pii/S1367593116301910},
local-url = {file://localhost/Users/colinrathbun/Documents/Library.papers3/Articles/2017/Obexer/Curr.%20Opin.%20Chem.%20Biol.%202017%20Obexer.pdf},
file = {{Curr. Opin. Chem. Biol. 2017 Obexer.pdf:/Users/colinrathbun/Documents/Library.papers3/Articles/2017/Obexer/Curr. Opin. Chem. Biol. 2017 Obexer.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1016/j.cbpa.2017.02.020}}
}

