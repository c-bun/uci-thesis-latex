file = {{Nucl. Acids Res. 2012 Garg.pdf:/Users/colinrathbun/Documents/Library.papers3/Articles/2012/Garg/Nucl. Acids Res. 2012 Garg.pdf:application/pdf;Nucl. Acids Res. 2012 Garg.pdf:/Users/colinrathbun/Documents/Library.papers3/Articles/2012/Garg/Nucl. Acids Res. 2012 Garg.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1093/nar/gks404}}
}

