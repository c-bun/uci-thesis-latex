journal = {Proceedings of the National Academy of Sciences of the United States of America},
year = {2016},
volume = {113},
number = {11},
pages = {E1442--51},
month = mar,
affiliation = {Department of Bioengineering, University of Pennsylvania, Philadelphia, PA 19104;},
doi = {10.1073/pnas.1509428113},
pmid = {26929367},
pmcid = {PMC4801262},
language = {English},
rating = {0},
date-added = {2017-06-08T17:38:09GMT},
date-modified = {2017-08-23T01:29:22GMT},
abstract = {Light-oxygen-voltage sensitive (LOV) flavoproteins are ubiquitous photoreceptors that mediate responses to environmental cues. Photosensory inputs are transduced into signaling outputs via structural rearrangements in sensor domains that consequently modulate the activity of an effector domain or multidomain clusters. Establishing the diversity in effector function and sensor-effector topology will inform what signaling mechanisms govern light-responsive behaviors across multiple kingdoms of life and how these signals are transduced. Here, we report the bioinformatics identification of over 6,700 candidate LOV domains (including over 4,000 previously unidentified sequences from plants and protists), and insights from their annotations for ontological function and structural arrangements. Motif analysis identified the sensors from ∼42 million ORFs, with strong statistical separation from other flavoproteins and non-LOV members of the structurally related Per-aryl hydrocarbon receptor nuclear translocator (ARNT)-Sim family. Conserved-domain analysis determined putative light-regulated function and multidomain topologies. We found that for certain effectors, sensor-effector linker length is discretized based on both phylogeny and the preservation of $\alpha$-helical heptad repeats within an extended coiled-coil linker structure. This finding suggests that preserving sensor-effector orientation is a key determinant of linker length, in addition to ancestry, in LOV signaling structure-function. We found a surprisingly high prevalence of effectors with functions previously thought to be rare among LOV proteins, such as regulators of G protein signaling, and discovered several previously unidentified effectors, such as lipases. This work highlights the value of applying genomic and transcriptomic technologies to diverse organisms to capture the structural and functional variation in photosensory proteins that are vastly important in adaptation, photobiology, and optogenetics.},
url = {http://www.pnas.org/lookup/doi/10.1073/pnas.1509428113},
local-url = {file://localhost/Users/colinrathbun/Documents/Library.papers3/Articles/2016/Glantz/Proceedings%20of%20the%20National%20Academy%20of%20Sciences%20of%20the%20United%20States%20of%20America%202016%20Glantz.pdf},
file = {{Proceedings of the National Academy of Sciences of the United States of America 2016 Glantz.pdf:/Users/colinrathbun/Documents/Library.papers3/Articles/2016/Glantz/Proceedings of the National Academy of Sciences of the United States of America 2016 Glantz.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1073/pnas.1509428113}}
}

