local-url = {file://localhost/Users/colinrathbun/Documents/Library.papers3/Articles/2010/Eir%C3%ADksd%C3%B3ttir/Bioconjugate%20Chem.%202010%20Eir%C3%ADksd%C3%B3ttir.pdf},
