uri = {\url{papers3://publication/doi/10.1021/ja8001919}}
}

@article{Beaver:2016ck,
author = {Beaver, Joshua E and Waters, Marcey L},
title = {{Molecular Recognition of Lys and Arg Methylation}},
journal = {ACS Chem. Biol.},
year = {2016},
volume = {11},
number = {3},
pages = {643--653},
month = mar,
publisher = {American Chemical Society},
affiliation = {Department of Chemistry, University of North Carolina , CB 3290, Chapel Hill, North Carolina 27599, United States.},
doi = {10.1021/acschembio.5b00996},
pmid = {26759915},
language = {English},
rating = {0},
date-added = {2017-06-08T18:23:50GMT},
date-modified = {2017-08-23T01:29:22GMT},
abstract = {A network of reader proteins and enzymes precisely controls gene transcription through the dynamic addition, removal, and recognition of post-translational modifications (PTMs) of histone tails. Histone PTMs work in concert with this network to regulate gene transcription through the histone code, and the dysregulation of PTM maintenance is linked to a large number of diseases, including many types of cancer. A wealth of research aims to elucidate the functions of this code, but our understanding of the effects of PTMs, specifically the methylation of lysine (Lys) and arginine (Arg), is lacking. The development of new tools to study PTMs relies on a sophisticated understanding of the mechanisms that drive protein and small molecule recognition in water. In this review, we outline the physical organic concepts that drive the molecular recognition of Lys and Arg methylation by reader proteins and draw comparisons to the binding mechanisms of small molecule receptors for methylated Lys and Arg that have been developed recently.},
url = {http://pubs.acs.org/doi/abs/10.1021/acschembio.5b00996},
local-url = {file://localhost/Users/colinrathbun/Documents/Library.papers3/Articles/2016/Beaver/ACS%20Chem.%20Biol.%202016%20Beaver.pdf},
file = {{ACS Chem. Biol. 2016 Beaver.pdf:/Users/colinrathbun/Documents/Library.papers3/Articles/2016/Beaver/ACS Chem. Biol. 2016 Beaver.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1021/acschembio.5b00996}}
}

