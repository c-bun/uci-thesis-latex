year = {2009},
volume = {106},
number = {37},
pages = {15599--15603},
month = sep,
publisher = {National Acad Sciences},
affiliation = {Research Institute for Cell Engineering, National Institute of Advanced Industrial Science and Technology, 1-8-31 Midorigaoka, Ikeda 563-8577, Japan.},
