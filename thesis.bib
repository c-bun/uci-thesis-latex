@article{Craig:1991uh,
author = {Craig, F F and Simmonds, A C and Watmore, D and MCCAPRA, F and White, M R},
title = {{Membrane-permeable luciferin esters for assay of firefly luciferase in live intact cells.}},
journal = {Biochem. J.},
year = {1991},
volume = {276 ( Pt 3)},
number = {Pt 3},
pages = {637--641},
month = jun,
publisher = {Portland Press Ltd},
affiliation = {Amersham International plc, Little Chalfont, Bucks, U.K.},
pmid = {2064603},
pmcid = {PMC1151052},
language = {English},
read = {Yes},
rating = {0},
date-added = {2015-02-27T20:38:15GMT},
date-modified = {2017-04-27T20:19:11GMT},
abstract = {Five esters of luciferin were synthesized and compared with native luciferin as substrates for firefly luciferase expressed in live intact mammalian cells. The esters themselves were not substrates for purified luciferase, but four were substrates for a purified esterase and all appeared to be hydrolysed to luciferin within mammalian cells. At a substrate concentration of 0.01 mM, the peak luminescence from the cos cells expressing luciferase was up to 6-fold greater with the esters than with unmodified luciferin. At 0.1 mM, the difference between luciferin and the esters was decreased. The kinetics of the luminescent signal with the different luciferin esters varied significantly, indicating possible differences in the rates of uptake, breakdown and enzyme inhibition. The esters did not support luminescence from Escherichia coli cells expressing firefly luciferase, suggesting a lack of appropriate esterase activity in this particular strain. The esters could be useful for the assay of luciferase expression in intact mammalian cells when luciferin levels are limiting, for example in tissues, and in plants. Alternative luciferin derivatives may allow further improvements in sensitivity.},
url = {/pmc/articles/PMC1151052/?report=abstract},
local-url = {file://localhost/Users/colinrathbun/Documents/Library.papers3/Articles/1991/Craig/Biochem.%20J.%201991%20Craig.pdf},
file = {{Biochem. J. 1991 Craig.pdf:/Users/colinrathbun/Documents/Library.papers3/Articles/1991/Craig/Biochem. J. 1991 Craig.pdf:application/pdf}},
uri = {\url{papers3://publication/uuid/3531BDA2-11A4-40DE-AB8E-7782E238CEC4}}
}

@article{Wang:2016cw,
author = {Wang, Yanxin J and Szantai-Kis, D Miklos and Petersson, E James},
title = {{Chemoselective modifications for the traceless ligation of thioamide-containing peptides and proteins}},
journal = {Org. Biomol. Chem.},
year = {2016},
volume = {14},
number = {26},
pages = {6262--6269},
month = jun,
publisher = {The Royal Society of Chemistry},
doi = {10.1039/C6OB01020B},
language = {English},
read = {Yes},
rating = {0},
date-added = {2017-01-05T00:46:11GMT},
date-modified = {2017-08-23T01:29:22GMT},
abstract = { Thioamides are single-atom substitutions of canonical amide bonds, and have been proven to be versatile and minimally perturbing probes in protein folding studies. Previously, our group showed that thioamides can be incorporated into proteins by native chemical ligation (NCL) with Cys as a ligation handle. In this study, we report the expansion of this strategy into non-Cys ligation sites, utilizing radical initiated desulfurization to {\textquotedblleft}erase{\textquotedblright} the side chain thiol after ligation. The reaction exhibited high chemoselectivity against thioamides, which can be further enhanced with thioacetamide as a sacrificial scavenger. As a proof-of-concept example, we demonstrated the incorporation of a thioamide probe into a 56 amino acid protein, the B1 domain of Protein G (GB1). Finally, we showed that the method can be extended to $\beta$-thiol amino acid analogs and selenocysteine.},
url = {http://pubs.rsc.org/en/content/articlehtml/2016/ob/c6ob01020b},
local-url = {file://localhost/Users/colinrathbun/Documents/Library.papers3/Articles/2016/Wang/Org.%20Biomol.%20Chem.%202016%20Wang.pdf},
file = {{Org. Biomol. Chem. 2016 Wang.pdf:/Users/colinrathbun/Documents/Library.papers3/Articles/2016/Wang/Org. Biomol. Chem. 2016 Wang.pdf:application/pdf;Org. Biomol. Chem. 2016 Wang.pdf:/Users/colinrathbun/Documents/Library.papers3/Articles/2016/Wang/Org. Biomol. Chem. 2016 Wang.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1039/C6OB01020B}}
}

