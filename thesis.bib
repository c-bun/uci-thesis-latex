@article{Tran:2013em,
author = {Tran, Vy and Moser, Lindsey A and Poole, Daniel S and Mehle, Andrew},
title = {{Highly Sensitive Real-Time In Vivo Imaging of an Influenza Reporter Virus Reveals Dynamics of Replication and Spread}},
journal = {J. Virol.},
year = {2013},
volume = {87},
number = {24},
pages = {13321--13329},
month = dec,
publisher = {American Society for Microbiology},
affiliation = {Medical Microbiology and Immunology, University of Wisconsin-Madison, Madison, Wisconsin, USA.},
doi = {10.1128/JVI.02381-13},
