journal = {Proceedings of the National Academy of Sciences of the United States of America},
year = {2014},
volume = {111},
number = {1},
pages = {433--438},
month = jan,
publisher = {National Acad Sciences},
affiliation = {Division of Chemistry and Chemical Engineering, Department of Bioengineering, and Proteome Exploration Laboratory, Beckman Institute, California Institute of Technology, Pasadena, CA 91125.},
