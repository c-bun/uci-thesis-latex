journal = {Biosci. Rep.},
year = {2000},
volume = {20},
number = {1},
pages = {21--30},
month = feb,
affiliation = {M.V. Lomonosov Moscow State University, School of Chemistry, Russia. eid@enzyme.chem.msu.ru},
