@article{GermainGenevois:2016cu,
author = {Germain-Genevois, Coralie and Garandeau, Olivia and Couillaud, Franck},
title = {{Detection of Brain Tumors and Systemic Metastases Using NanoLuc and Fluc for Dual Reporter Imaging}},
