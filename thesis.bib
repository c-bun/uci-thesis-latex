@article{Heffern:2016hc,
author = {Heffern, Marie C and Park, Hyo Min and Au-Yeung, Ho Yu and Van de Bittner, Genevieve C and Ackerman, Cheri M and Stahl, Andreas and Chang, Christopher J},
title = {{In vivo bioluminescence imaging reveals copper deficiency in a murine model of nonalcoholic fatty liver disease.}},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
year = {2016},
volume = {113},
number = {50},
pages = {14219--14224},
month = dec,
publisher = {National Acad Sciences},
affiliation = {Department of Chemistry, University of California, Berkeley, CA 94720.},
doi = {10.1073/pnas.1613628113},
pmid = {27911810},
pmcid = {PMC5167165},
language = {English},
read = {Yes},
rating = {0},
date-added = {2017-04-18T22:00:36GMT},
date-modified = {2017-08-23T01:29:21GMT},
abstract = {Copper is a required metal nutrient for life, but global or local alterations in its homeostasis are linked to diseases spanning genetic and metabolic disorders to cancer and neurodegeneration. Technologies that enable longitudinal in vivo monitoring of dynamic copper pools can help meet the need to study the complex interplay between copper status, health, and disease in the same living organism over time. Here, we present the synthesis, characterization, and in vivo imaging applications of Copper-Caged Luciferin-1 (CCL-1), a bioluminescent reporter for tissue-specific copper visualization in living animals. CCL-1 uses a selective copper(I)-dependent oxidative cleavage reaction to release d-luciferin for subsequent bioluminescent reaction with firefly luciferase. The probe can detect physiological changes in labile Cu(+) levels in live cells and mice under situations of copper deficiency or overload. Application of CCL-1 to mice with liver-specific luciferase expression in a diet-induced model of nonalcoholic fatty liver disease reveals onset of hepatic copper deficiency and altered expression levels of central copper trafficking proteins that accompany symptoms of glucose intolerance and weight gain. The data connect copper dysregulation to metabolic liver disease and provide a starting point for expanding the toolbox of reactivity-based chemical reporters for cell- and tissue-specific in vivo imaging.},
url = {http://www.pnas.org/content/113/50/14219.full},
local-url = {file://localhost/Users/colinrathbun/Documents/Library.papers3/Articles/2016/Heffern/Proceedings%20of%20the%20National%20Academy%20of%20Sciences%20of%20the%20United%20States%20of%20America%202016%20Heffern.pdf},
file = {{Proceedings of the National Academy of Sciences of the United States of America 2016 Heffern.pdf:/Users/colinrathbun/Documents/Library.papers3/Articles/2016/Heffern/Proceedings of the National Academy of Sciences of the United States of America 2016 Heffern.pdf:application/pdf;Proceedings of the National Academy of Sciences of the United States of America 2016 Heffern.pdf:/Users/colinrathbun/Documents/Library.papers3/Articles/2016/Heffern/Proceedings of the National Academy of Sciences of the United States of America 2016 Heffern.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1073/pnas.1613628113}}
}

