title = {{N-$\alpha$-Benzoyl-N5-(2-Chloro-1-Iminoethyl)-l-Ornithine Amide, a Protein Arginine Deiminase Inhibitor, Reduces the Severity of Murine Collagen-Induced Arthritis}},
journal = {The Journal of Immunology},
year = {2011},
volume = {186},
number = {7},
pages = {4396--4404},
month = apr,
publisher = {American Association of Immunologists},
affiliation = {Division of Rheumatology, Department of Medicine, University of Colorado School of Medicine, Aurora, CO 80045, USA.},
doi = {10.4049/jimmunol.1001620},
pmid = {21346230},
pmcid = {PMC3085980},
language = {English},
read = {Yes},
rating = {0},
date-added = {2017-02-08T17:39:45GMT},
date-modified = {2017-08-23T01:29:21GMT},
abstract = {Rheumatoid arthritis is associated with the development of autoantibodies to citrullinated self-proteins. Citrullinated synovial proteins, which are generated via the actions of the protein arginine deiminases (PADs), are known to develop in the murine collagen-induced arthritis (CIA) model of inflammatory arthritis. Given these findings, we evaluated whether N-$\alpha$-benzoyl-N5-(2-chloro-1-iminoethyl)-l-ornithine amide (Cl-amidine), a recently described pan-PAD inhibitor, could affect the development of arthritis and autoimmunity by treating mice in the CIA model with Cl-amidine on days 0{\textendash}35. Cl-amidine treatment reduced total synovial and serum citrullination, decreased clinical disease activity by ∼50%, and significantly decreased IgG2a anti-mouse type II collagen Abs. Additionally, histopathology scores and total complement C3 deposition were significantly lower in Cl-amidine{\textendash}treated mice compared with vehicle controls. Synovial microarray analyses demonstrated decreased IgG reactivity to several native and citrullinated epitopes compared with vehicle controls. Cl-amidine treatment had no ameliorative effect on collagen Ab-induced arthritis, suggesting its primary protective mechanism was not mediated through effector pathways. Reduced levels of citrullinated synovial proteins observed in mice treated with Cl-amidine are consistent with the notion that Cl-amidine derives its efficacy from its ability to inhibit the deiminating activity of PADs. In total, these results suggested that PADs are necessary participants in the autoimmune and subsequent inflammatory processes in CIA. Cl-amidine may represent a novel class of disease-modifying agents that modulate aberrant citrullination, and perhaps other immune processes, necessary for the development of inflammatory arthritis.},
url = {http://www.jimmunol.org/cgi/doi/10.4049/jimmunol.1001620},
local-url = {file://localhost/Users/colinrathbun/Documents/Library.papers3/Articles/2011/Van%20C%20Willis/The%20Journal%20of%20Immunology%202011%20Van%20C%20Willis.pdf},
file = {{The Journal of Immunology 2011 Van C Willis.pdf:/Users/colinrathbun/Documents/Library.papers3/Articles/2011/Van C Willis/The Journal of Immunology 2011 Van C Willis.pdf:application/pdf;The Journal of Immunology 2011 Van C Willis.pdf:/Users/colinrathbun/Documents/Library.papers3/Articles/2011/Van C Willis/The Journal of Immunology 2011 Van C Willis.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.4049/jimmunol.1001620}}
}

