author = {Islam, Kabirul and Chen, Yuling and Wu, Hong and Bothwell, Ian R and Blum, Gil J and Zeng, Hong and Dong, Aiping and Zheng, Weihong and Min, Jinrong and Deng, Haiteng and Luo, Minkui},
title = {{Defining efficient enzyme-cofactor pairs for bioorthogonal profiling of protein methylation.}},
