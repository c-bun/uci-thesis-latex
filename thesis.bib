@article{Schaub:2015hk,
author = {Schaub, Franz X and Reza, Md Shamim and Flaveny, Colin A and Li, Weimin and Musicant, Adele M and Hoxha, Sany and Guo, Min and Cleveland, John L and Amelio, Antonio L},
title = {{Fluorophore-NanoLuc BRET Reporters Enable Sensitive In Vivo Optical Imaging and Flow Cytometry for Monitoring Tumorigenesis}},
journal = {Cancer. Res.},
year = {2015},
volume = {75},
number = {23},
pages = {5023--5033},
month = dec,
publisher = {American Association for Cancer Research},
affiliation = {Department of Tumor Biology, Moffitt Cancer Center and Research Institute, Tampa, Florida.},
doi = {10.1158/0008-5472.CAN-14-3538},
pmid = {26424696},
pmcid = {PMC4668208},
language = {English},
read = {Yes},
rating = {0},
date-added = {2017-04-28T16:20:50GMT},
date-modified = {2017-08-23T01:29:23GMT},
abstract = {Fluorescent proteins are widely used to study molecular and cellular events, yet this traditionally relies on delivery of excitation light, which can trigger autofluorescence, photoxicity, and photobleaching, impairing their use in vivo . Accordingly, chemiluminescent light sources such as those generated by luciferases have emerged, as they do not require excitation light. However, current luciferase reporters lack the brightness needed to visualize events in deep tissues. We report the creation of chimeric eGFP-NanoLuc (GpNLuc) and LSSmOrange-NanoLuc (OgNLuc) fusion reporter proteins coined LumiFluors, which combine the benefits of eGFP or LSSmOrange fluorescent proteins with the bright, glow-type bioluminescent light generated by an enhanced small luciferase subunit (NanoLuc) of the deep-sea shrimp Oplophorus gracilirostris . The intramolecular bioluminescence resonance energy transfer that occurs between NanoLuc and the fused fluorophore generates the brightest bioluminescent signal known to date, including improved intensity, sensitivity, and durable spectral properties, thereby dramatically reducing image acquisition times and permitting highly sensitive in vivo imaging. Notably, the self-illuminating and bifunctional nature of these LumiFluor reporters enables greatly improved spatiotemporal monitoring of very small numbers of tumor cells via in vivo optical imaging and also allows the isolation and analyses of single cells by flow cytometry. Thus, LumiFluor reporters are inexpensive, robust, noninvasive tools that allow for markedly improved in vivo optical imaging of tumorigenic processes. Cancer Res; 75(23); 5023{\textendash}33. {\textcopyright}2015 AACR .},
url = {http://cancerres.aacrjournals.org/cgi/doi/10.1158/0008-5472.CAN-14-3538},
local-url = {file://localhost/Users/colinrathbun/Documents/Library.papers3/Articles/2015/Schaub/Cancer.%20Res.%202015%20Schaub.pdf},
file = {{Cancer. Res. 2015 Schaub.pdf:/Users/colinrathbun/Documents/Library.papers3/Articles/2015/Schaub/Cancer. Res. 2015 Schaub.pdf:application/pdf;Cancer. Res. 2015 Schaub.pdf:/Users/colinrathbun/Documents/Library.papers3/Articles/2015/Schaub/Cancer. Res. 2015 Schaub.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1158/0008-5472.CAN-14-3538}}
}

