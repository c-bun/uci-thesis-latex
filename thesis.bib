@article{VandeBittner:2010bi,
author = {Van de Bittner, Genevieve C and Dubikovskaya, Elena A and Bertozzi, Carolyn R and Chang, Christopher J},
title = {{In vivo imaging of hydrogen peroxide production in a murine tumor model with a chemoselective bioluminescent reporter.}},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
year = {2010},
volume = {107},
number = {50},
pages = {21316--21321},
month = dec,
publisher = {National Acad Sciences},
affiliation = {Department of Chemistry, Howard Hughes Medical Institute, University of California, Berkeley, CA 94720, USA.},
doi = {10.1073/pnas.1012864107},
pmid = {21115844},
pmcid = {PMC3003011},
language = {English},
rating = {0},
date-added = {2016-07-08T02:32:28GMT},
date-modified = {2017-08-23T01:29:21GMT},
abstract = {Living organisms produce hydrogen peroxide (H(2)O(2)) to kill invading pathogens and for cellular signaling, but aberrant generation of this reactive oxygen species is a hallmark of oxidative stress and inflammation in aging, injury, and disease. The effects of H(2)O(2) on the overall health of living animals remain elusive, in part owing to a dearth of methods for studying this transient small molecule in vivo. Here we report the design, synthesis, and in vivo applications of Peroxy Caged Luciferin-1 (PCL-1), a chemoselective bioluminescent probe for the real-time detection of H(2)O(2) within living animals. PCL-1 is a boronic acid-caged firefly luciferin molecule that selectively reacts with H(2)O(2) to release firefly luciferin, which triggers a bioluminescent response in the presence of firefly luciferase. The high sensitivity and selectivity of PCL-1 for H(2)O(2), combined with the favorable properties of bioluminescence for in vivo imaging, afford a unique technology for real-time detection of basal levels of H(2)O(2) generated in healthy, living mice. Moreover, we demonstrate the efficacy of PCL-1 for monitoring physiological fluctuations in H(2)O(2) levels by directly imaging elevations in H(2)O(2) within testosterone-stimulated tumor xenografts in vivo. The ability to chemoselectively monitor H(2)O(2) fluxes in real time in living animals offers opportunities to dissect H(2)O(2)'s disparate contributions to health, aging, and disease.},
url = {http://www.pnas.org/cgi/doi/10.1073/pnas.1012864107},
local-url = {file://localhost/Users/colinrathbun/Documents/Library.papers3/Articles/2010/Van%20de%20Bittner/Proceedings%20of%20the%20National%20Academy%20of%20Sciences%20of%20the%20United%20States%20of%20America%202010%20Van%20de%20Bittner.pdf},
file = {{Proceedings of the National Academy of Sciences of the United States of America 2010 Van de Bittner.pdf:/Users/colinrathbun/Documents/Library.papers3/Articles/2010/Van de Bittner/Proceedings of the National Academy of Sciences of the United States of America 2010 Van de Bittner.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1073/pnas.1012864107}}
}

