@article{BRANCHINI:2001gr,
author = {BRANCHINI, B R and Magyar, R A and Murtiashaw, M H and Portier, N C},
title = {{The role of active site residue arginine 218 in firefly luciferase bioluminescence}},
journal = {Biochemistry},
year = {2001},
volume = {40},
number = {8},
