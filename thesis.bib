year = {2015},
volume = {14},
number = {9},
pages = {603--622},
month = sep,
publisher = {Nature Research},
affiliation = {GlaxoSmithKline, 1250 South Collegeville Road, Collegeville, Pennsylvania 19426, USA.},
doi = {10.1038/nrd4596},
