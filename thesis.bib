@article{RN165,
author = {Mofford, David M and Reddy, Gadarla Randheer and Miller, Stephen C},
title = {{Latent luciferase activity in the fruit fly revealed by a synthetic luciferin.}},
journal = {Proc. Natl. Acad. Sci. U.S.A.},
year = {2014},
volume = {111},
number = {12},
pages = {4443--4448},
month = mar,
publisher = {National Acad Sciences},
affiliation = {Department of Biochemistry and Molecular Pharmacology, University of Massachusetts Medical School, Worcester, MA 01605.},
doi = {10.1073/pnas.1319300111},
pmid = {24616520},
pmcid = {PMC3970535},
language = {English},
read = {Yes},
rating = {0},
date-added = {2016-07-08T01:54:31GMT},
date-modified = {2017-05-03T00:20:55GMT},
abstract = {Beetle luciferases are thought to have evolved from fatty acyl-CoA synthetases present in all insects. Both classes of enzymes activate fatty acids with ATP to form acyl-adenylate intermediates, but only luciferases can activate and oxidize d-luciferin to emit light. Here we show that the Drosophila fatty acyl-CoA synthetase CG6178, which cannot use d-luciferin as a substrate, is able to catalyze light emission from the synthetic luciferin analog CycLuc2. Bioluminescence can be detected from the purified protein, live Drosophila Schneider 2 cells, and from mammalian cells transfected with CG6178. Thus, the nonluminescent fruit fly possesses an inherent capacity for bioluminescence that is only revealed upon treatment with a xenobiotic molecule. This result expands the scope of bioluminescence and demonstrates that the introduction of a new substrate can unmask latent enzymatic activity that differs significantly from an enzyme's normal function without requiring mutation.},
url = {http://www.pnas.org/cgi/doi/10.1073/pnas.1319300111},
local-url = {file://localhost/Users/colinrathbun/Documents/Library.papers3/Articles/2014/Mofford/Proc%20Natl%20Acad%20Sci%20USA%202014%20Mofford.pdf},
file = {{Proc Natl Acad Sci USA 2014 Mofford.pdf:/Users/colinrathbun/Documents/Library.papers3/Articles/2014/Mofford/Proc Natl Acad Sci USA 2014 Mofford.pdf:application/pdf;Proc Natl Acad Sci USA 2014 Mofford.pdf:/Users/colinrathbun/Documents/Library.papers3/Articles/2014/Mofford/Proc Natl Acad Sci USA 2014 Mofford.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1073/pnas.1319300111}}
}

