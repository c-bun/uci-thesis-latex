title = {{Mutagenesis of solvent-exposed amino acids in Photinus pyralis luciferase improves thermostability and pH-tolerance.}},
journal = {Biochem. J.},
year = {2006},
volume = {397},
number = {2},
pages = {305--312},
month = jul,
publisher = {Portland Press Ltd.},
affiliation = {Institute of Biotechnology, University of Cambridge, Tennis Court Road, Cambridge CB2 1QT, UK. E.Law@biotech.cam.ac.uk},
