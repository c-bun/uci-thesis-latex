year = {2015},
volume = {137},
number = {27},
pages = {8656--8659},
month = jul,
publisher = {American Chemical Society},
doi = {10.1021/jacs.5b02774},
pmid = {26098396},
language = {English},
read = {Yes},
rating = {0},
date-added = {2015-11-09T19:50:31GMT},
date-modified = {2017-05-01T22:08:04GMT},
abstract = {Cell-cell interactions underlie fundamental biological processes but remain difficult to visualize over long times and large distances in tissues and live organisms. Bioluminescence imaging with luciferase-luciferin pairs is sufficiently sensitive to image cells in vivo but lacks the spatial resolution to identify cellular locations and interactions. To repurpose this technology for visualizing cellular networks, we developed a "caged" luciferin that produces light only when cells are in close contact. This molecule comprises a nitroaromatic core that can be selectively reduced ("uncaged") by one cell type, liberating a luciferin that can be selectively consumed by neighboring, luciferase-expressing cells. When the two cell types are in contact, robust light emission is observed. This imaging strategy will enable the noninvasive visualization of cell-cell interactions relevant to organismal biology.},
url = {http://pubs.acs.org/doi/abs/10.1021/jacs.5b02774},
local-url = {file://localhost/Users/colinrathbun/Documents/Library.papers3/Articles/2015/Porterfield/J.%20Am.%20Chem.%20Soc.%202015%20Porterfield.pdf},
file = {{J. Am. Chem. Soc. 2015 Porterfield.pdf:/Users/colinrathbun/Documents/Library.papers3/Articles/2015/Porterfield/J. Am. Chem. Soc. 2015 Porterfield.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1021/jacs.5b02774}}
}

