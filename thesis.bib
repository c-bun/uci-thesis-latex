@article{Kuchimaru:2016eba,
author = {Kuchimaru, Takahiro and Iwano, Satoshi and Kiyama, Masahiro and Mitsumata, Shun and Kadonosono, Tetsuya and Niwa, Haruki and Maki, Shojiro and Kizaka-Kondoh, Shinae},
title = {{A luciferin analogue generating near-infrared bioluminescence achieves highly sensitive deep-tissue imaging.}},
journal = {Nat. Commun.},
year = {2016},
volume = {7},
pages = {11856},
publisher = {Nature Publishing Group},
affiliation = {School of Life Science and Technology, Tokyo Institute of Technology, 4259, Nagatsuta, Midori-ku, Yokohama 226-8501, Japan.},
