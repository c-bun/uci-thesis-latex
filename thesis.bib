@article{Shis:2013fu,
author = {Shis, David L and Bennett, Matthew R},
title = {{Library of synthetic transcriptional AND gates built with split T7 RNA polymerase mutants.}},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
year = {2013},
volume = {110},
number = {13},
pages = {5028--5033},
month = mar,
publisher = {National Acad Sciences},
affiliation = {Department of Biochemistry and Cell Biology, Rice University, Houston, TX 77005, USA.},
doi = {10.1073/pnas.1220157110},
pmid = {23479654},
pmcid = {PMC3612686},
language = {English},
read = {Yes},
rating = {0},
date-added = {2017-04-06T17:05:46GMT},
date-modified = {2017-04-27T20:19:11GMT},
abstract = {The construction of synthetic gene circuits relies on our ability to engineer regulatory architectures that are orthogonal to the host's native regulatory pathways. However, as synthetic gene circuits become larger and more complicated, we are limited by the small number of parts, especially transcription factors, that work well in the context of the circuit. The current repertoire of transcription factors consists of a limited selection of activators and repressors, making the implementation of transcriptional logic a complicated and component-intensive process. To address this, we modified bacteriophage T7 RNA polymerase (T7 RNAP) to create a library of transcriptional AND gates for use in Escherichia coli by first splitting the protein and then mutating the DNA recognition domain of the C-terminal fragment to alter its promoter specificity. We first demonstrate that split T7 RNAP is active in vivo and compare it with full-length enzyme. We then create a library of mutant split T7 RNAPs that have a range of activities when used in combination with a complimentary set of altered T7-specific promoters. Finally, we assay the two-input function of both wild-type and mutant split T7 RNAPs and find that regulated expression of the N- and C-terminal fragments of the split T7 RNAPs creates AND logic in each case. This work demonstrates that mutant split T7 RNAP can be used as a transcriptional AND gate and introduces a unique library of components for use in synthetic gene circuits.},
url = {http://www.pnas.org/content/110/13/5028.full},
local-url = {file://localhost/Users/colinrathbun/Documents/Library.papers3/Articles/2013/Shis/Proceedings%20of%20the%20National%20Academy%20of%20Sciences%20of%20the%20United%20States%20of%20America%202013%20Shis.pdf},
file = {{Proceedings of the National Academy of Sciences of the United States of America 2013 Shis.pdf:/Users/colinrathbun/Documents/Library.papers3/Articles/2013/Shis/Proceedings of the National Academy of Sciences of the United States of America 2013 Shis.pdf:application/pdf;Proceedings of the National Academy of Sciences of the United States of America 2013 Shis.pdf:/Users/colinrathbun/Documents/Library.papers3/Articles/2013/Shis/Proceedings of the National Academy of Sciences of the United States of America 2013 Shis.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1073/pnas.1220157110}}
}

