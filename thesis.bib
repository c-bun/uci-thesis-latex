@article{Melnyk:2015ct,
author = {Melnyk, James E and Mohanan, Vishnu and Schaefer, Amy K and Hou, Ching-Wen and Grimes, Catherine Leimkuhler},
title = {{Peptidoglycan Modifications Tune the Stability and Function of the Innate Immune Receptor Nod2.}},
journal = {J. Am. Chem. Soc.},
year = {2015},
volume = {137},
number = {22},
pages = {6987--6990},
month = jun,
publisher = {American Chemical Society},
affiliation = {{\dag}Department of Chemistry and Biochemistry, and {\textsection}Department of Biological Sciences, University of Delaware, Newark, Delaware 19716, United States.},
doi = {10.1021/jacs.5b01607},
pmid = {26035228},
pmcid = {PMC4878121},
language = {English},
rating = {0},
date-added = {2017-06-08T18:14:14GMT},
date-modified = {2017-08-23T01:29:21GMT},
abstract = {Natural modifications of peptidoglycan modulate the innate immune response. Peptidoglycan derivatives activate this response via the intracellular innate immune receptor, Nod2. To probe how these modifications alter the response, a novel and efficient carbohydrate synthesis was developed to allow for late-stage modification of the amine at the 2-position. Modification of the carbohydrate was found to be important for stabilizing Nod2 and generating the proper response. The native Nod2 ligands demonstrate a significant increase in the cellular stability of Nod2. Moreover, changing the identity of the natural ligands at the carbohydrate 2-position allows for the Nod2-dependent immune response to be either up-regulated or down-regulated. The ligand structure can be adjusted to tune the Nod2 response, suggesting that other innate immune receptors and their ligands could use a similar strategy.},
url = {http://pubs.acs.org/doi/abs/10.1021/jacs.5b01607},
local-url = {file://localhost/Users/colinrathbun/Documents/Library.papers3/Articles/2015/Melnyk/J.%20Am.%20Chem.%20Soc.%202015%20Melnyk.pdf},
file = {{J. Am. Chem. Soc. 2015 Melnyk.pdf:/Users/colinrathbun/Documents/Library.papers3/Articles/2015/Melnyk/J. Am. Chem. Soc. 2015 Melnyk.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1021/jacs.5b01607}}
}

